{
  "expo": {
    "name": "Renttar",
    "slug": "renttar",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/Onboarding.jpg",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.yourcompany.renttar"
    },
    "android": {
      "package": "com.yourcompany.renttar"
    },
    "plugins": [
      "expo-image-picker"
    ]
  }
}